#include <stdio.h>
#include <stdio.h>


int main() {
	int a , b;
	printf ("Enter Your Num : \r\n") ;
	fflush(stdin); fflush(stdout);
	scanf ( "%d  " , &a , &b);
	printf (" You Entered (%d) " , a+b ) ;


	return 0 ;

}

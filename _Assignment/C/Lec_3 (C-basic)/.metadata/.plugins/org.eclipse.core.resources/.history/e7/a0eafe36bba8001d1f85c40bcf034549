#include <stdio.h>
#include <stdio.h>


int main() {
	char a ;
	printf ("Enter Your char :") ;
	fflush(stdin); fflush(stdout);
	scanf ( "%c",&a);
	printf (" %c " , 'a' );

	return 0 ;

}

#include <stdio.h>
#include <stdlib.h>

int main() {
	 char c[100], ch;
	 int cunt=0;
	 printf("Enter String :");
	 fflush(stdin); fflush(stdout);
	 gets(c);
	 printf("Enter Character to frequency: ");
	 fflush(stdin); fflush(stdout);
	 scanf("%c",&ch);
	 for(int i=0 ; c[i] != '\0' ; i++ ){
		 if (ch == c[i]) cunt++;
	 }
	 printf("Freq of %c = %d" ,ch,cunt);
	return 0;
}

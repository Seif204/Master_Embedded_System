#include <stdio.h>
#include <stdlib.h>

int main() {

	int a,b;
	printf("Enter Rows & Column of Matrix : ");
	fflush(stdin);fflush(stdout);
	scanf("%d%d" ,&a,&b);
	int x[a][b];
	for(int i=0;i<a;i++){
		for(int j=0;j<b;j++){
			printf("Enter x%d%d =", i+1,j+1);
			fflush(stdin);fflush(stdout);
			scanf("%d" ,x[i][j]);
		}
	}
	printf("Entered Matrix: ");
	for(int i=0;i<a;i++){
		for(int j=0;j<b;j++){
			printf("Enter %d =", x[i][j]);
		}
		printf("\n");
	}


	return 0;
}

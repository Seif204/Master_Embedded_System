#include <stdio.h>


int main() {
	int a;
	printf ("Enter Your Num : \r\n") ;
	fflush(stdin); fflush(stdout);
	scanf ("%d " , &a);
	printf (" You Entered (%d) " , a ) ;


	return 0 ;
}

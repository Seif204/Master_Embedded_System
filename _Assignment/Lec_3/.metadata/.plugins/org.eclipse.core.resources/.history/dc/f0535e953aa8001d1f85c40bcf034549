#include <stdio.h>
#include <stdio.h>


int main() {
	int a-0 , b-0;
	printf ("Enter Your 2 Num :") ;
	fflush(stdin); fflush(stdout);
	scanf ( " %d %d " , &a , &b);
	printf (" You Entered %d\r\n " , a+b );

	return 0 ;

}

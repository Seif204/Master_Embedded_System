#include <stdio.h>
#include <stdlib.h>

int main() {

	float a[2][2] , b[2][2], sum[2][2];
	printf("Enter The Elements of 1st Array :");
	for( int i=0;i<2;i++){
		for (int j=0 ;j<2;j++){
         printf("Enter a%d%d",i+1,j+1);
         scanf("%f",&a[i][j]);
		}
	}
	return 0;
}
